    "textMateRules": [
      {
        "scope": "punctuation.definition.string.begin",
        "settings": {
          "foreground": "#ffd986" // Color para las comillas de apertura
        }
      },
      {
        "scope": "punctuation.definition.string.end",
        "settings": {
          "foreground": "#ffd986" // Color para las comillas de cierre
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#ff0000" // Color para el símbolo &
        }
      },
      {
        "scope": "keyword.operator.assignment",
        "settings": {
          "foreground": "#ff7300" // Color para el símbolo &
        }
      },
      {
        "scope": "punctuation.separator.colon",
        "settings": {
          "foreground": "#ff9100" // Color para el símbolo :
        }
      },
      {
        "scope": "punctuation.terminator.statement",
        "settings": {
          "foreground": "#ffbb00" // Color para el símbolo ;
        }
      },
      {
        "scope": "punctuation.separator.continuation",
        "settings": {
          "foreground": "#ffe600" // Color para el símbolo ,
        }
      },
      {
        "scope": "punctuation",
        "settings": {
          "foreground": "#ffffff"
        }
      }
    ]
  },
