{
	"editor.tabCompletion": "on",
	"editor.guides.indentation": true,
	"editor.guides.highlightActiveIndentation": true,
	"editor.fontLigatures": true,
	"editor.overviewRulerBorder": false,
	"editor.hideCursorInOverviewRuler": true,
	"editor.guides.highlightActiveBracketPair": true,
	"editor.autoClosingQuotes": "always",
	"editor.guides.bracketPairs": false,
	"editor.autoIndent": "full",
	"explorer.confirmDelete": false,
	"workbench.editor.enablePreview": false,
	"chat.commandCenter.enabled": false,
	"workbench.navigationControl.enabled": false,
	"files.autoSave": "onFocusChange",
	"editor.wordWrap": "on",
	"editor.autoClosingOvertype": "always",
	"editor.autoClosingDelete": "always",
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"files.trimTrailingWhitespace": true,
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"editor.formatOnPaste": true,
	"window.titleBarStyle": "custom",
	"workbench.iconTheme": "material-icon-theme",
	"editor.scrollbar.horizontal": "hidden",
	"editor.scrollbar.vertical": "hidden",
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.scrollBeyondLastLine": false,
	"workbench.editor.showTabs": "multiple",
	"editor.formatOnSave": true,
	"prettier.bracketSpacing": true,
	"prettier.bracketSameLine": true,
	"prettier.arrowParens": "always",
	"prettier.trailingComma": "es5",
	"prettier.useTabs": true,
	"prettier.tabWidth": 2,
	"prettier.singleQuote": true,
	"prettier.semi": true,
	"prettier.htmlWhitespaceSensitivity": "strict",
	"security.workspace.trust.untrustedFiles": "open",
	"editor.padding.top": 20,
	"editor.lineHeight": 20,
	"editor.fontSize": 12.5,
	"editor.glyphMargin": true,
	"editor.lineNumbers": "on",
	"editor.padding.bottom": 20,
	"editor.stickyTabStops": true,
	"breadcrumbs.showArrays": false,
	"breadcrumbs.showBooleans": false,
	"breadcrumbs.showClasses": false,
	"breadcrumbs.showConstants": false,
	"breadcrumbs.showConstructors": false,
	"breadcrumbs.showEnumMembers": false,
	"breadcrumbs.showEnums": false,
	"breadcrumbs.showEvents": false,
	"breadcrumbs.showFields": false,
	"breadcrumbs.showFiles": false,
	"breadcrumbs.showVariables": false,
	"breadcrumbs.showTypeParameters": false,
	"breadcrumbs.showStructs": false,
	"breadcrumbs.showStrings": false,
	"breadcrumbs.showProperties": false,
	"breadcrumbs.showPackages": false,
	"breadcrumbs.showOperators": false,
	"breadcrumbs.showObjects": false,
	"breadcrumbs.showNumbers": false,
	"breadcrumbs.showNull": false,
	"breadcrumbs.showNamespaces": false,
	"breadcrumbs.showModules": false,
	"breadcrumbs.showMethods": false,
	"breadcrumbs.showInterfaces": false,
	"breadcrumbs.showFunctions": false,
	"workbench.colorCustomizations": {
		// "editor.background": "#0c052750",
		"sideBar.border": "#223323",
		"breadcrumbs.enabled": true,
		"breadcrumb.foreground": "#146bb3",
		"breadcrumb.background": "#000000",
		"breadcrumb.focusForeground": "#3998cf",
		"breadcrumb.activeSelectionForeground": "#00ffdd",
		"sideBar.foreground": "#6bad76",
		"sideBar.background": "#080220",
		"sideBarSectionHeader.foreground": "#84a3db",
		"sideBarSectionHeader.background": "#000000",
		"sideBarTitle.foreground": "#4e4e5f",
		"list.hoverForeground": "#00ff22",
		"list.hoverBackground": "#1c1c3a",
		"menu.foreground": "#6bad76",
		"menu.background": "#0c0527",
		"menu.selectionForeground": "#00ff22",
		"menu.selectionBackground": "#21213d",
		"menu.separatorBackground": "#84a3db",
		"tab.activeForeground": "#15ff00",
		"tab.inactiveForeground": "#309b59",
		"tab.hoverForeground": "#00ff22",
		"tab.hoverBackground": "#1c1c3a",
		"css.format.spaceAroundSelectorSeparator": true,
		"less.format.spaceAroundSelectorSeparator": true,
		"scss.format.spaceAroundSelectorSeparator": true,
		"files.trimTrailingWhitespace": true
	},
	"editor.tabSize": 2,
	"editor.indentSize": 2,
	"editor.insertSpaces": true,
	"editor.cursorBlinking": "expand",
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"workbench.tree.indent": 4,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.defaultColorDecorators": "always",
	"editor.defaultFormatter": "GitHub.copilot",
	"editor.overtypeCursorStyle": "line",
	"prettier.proseWrap": "always",
	"workbench.sideBar.location": "right",
	"editor.renderWhitespace": "none",
	"editor.definitionLinkOpensInPeek": true,
	"editor.letterSpacing": 0.5,
	"editor.fontFamily": "Cascadia Code",
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": "punctuation.definition.string.begin",
				"settings": {
					"foreground": "#ffffff",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "punctuation.definition.string.end",
				"settings": {
					"foreground": "#ffffff",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "keyword.operator",
				"settings": {
					"foreground": "#c8ff00",
					"fontStyle": ""
				}
			},
			{
				"scope": "keyword.operator.assignment",
				"settings": {
					"foreground": "#ffee00",
					"fontStyle": ""
				}
			},
			{
				"scope": "punctuation.separator.colon",
				"settings": {
					"foreground": "#ff6600",
					"fontStyle": ""
				}
			},
			{
				"scope": "punctuation.terminator.statement",
				"settings": {
					"foreground": "#ffff00",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "punctuation.separator.continuation",
				"settings": {
					"foreground": "#ff6600",
					"fontStyle": ""
				}
			},
			{
				"scope": "punctuation",
				"settings": {
					"foreground": "#ffffff",
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"keyword.other.new",
					"keyword.other",
					"keyword.control",
					"entity.name.function",
					"support.function"
				],
				"settings": {
					"foreground": "#48b964",
					"fontStyle": ""
				}
			},
			{
				"scope": ["variable"],
				"settings": {
					"foreground": "#03ff03"
					// "fontStyle": "bold"
				}
			}
		]
	},
	"editor.linkedEditing": true,
	"window.commandCenter": false,
	"window.title": "> / ${activeEditorMedium} / ",
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.editor.openPositioning": "last",
	"workbench.editor.wrapTabs": true,
	"workbench.editor.centeredLayoutFixedWidth": true,
	"[typescriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"diffEditor.experimental.showEmptyDecorations": false,
	"diffEditor.codeLens": true,
	"diffEditor.maxFileSize": 0,
	"diffEditor.wordWrap": "on",
	"editor.minimap.maxColumn": 1500,
	"editor.minimap.sectionHeaderLetterSpacing": 150,
	"editor.minimap.enabled": false,
	"editor.suggest.localityBonus": true,
	"editor.suggest.preview": true,
	"files.defaultLanguage": "${activeEditorLanguage}",
	"workbench.startupEditor": "none",
	"workbench.colorTheme": "Platzi Theme Green Mode"
}
