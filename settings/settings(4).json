{
  "editor.minimap.enabled": false,
  "git.enableSmartCommit": true,
  "git.decorations.enabled": false,
  "scm.diffDecorations": "none",
  "files.autoSave": "afterDelay",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "files.associations": {
    ".env*": "dotenv"
  },
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#FF000000"
          }
        }
      ]
    },
    "textMateRules": [
      {
        "scope": "punctuation.definition.tag.begin, punctuation.definition.tag.end",
        "settings": {
          "foreground": "#e82660"
        }
      },
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  // ...otras configuraciones...
  "workbench.colorCustomizations": {
    "editorSuggestWidget.foreground": "#89a1ff",
    // Colores para las líneas de indentación
    "editorIndentGuide.background1": "#5c5c5c40", // líneas inactivas
    "editorIndentGuide.activeBackground1": "#00ff7351", // línea activa
    "editor.lineHighlightBackground": "#082536",
    "editor.lineHighlightBorder": "default",
    "tree.indentGuidesStroke": "#89a1ff",
    "breadcrumb.foreground": "#00a6ff",
    "breadcrumbPicker.background": "#000000",
    "breadcrumb.activeSelectionForeground": "#1eff00",
    "breadcrumb.focusForeground": "#384571",
    "breadcrumb.background": "#061b33",
    "tab.hoverBackground": "#141474",
    "tab.activeForeground": "#01ff51",
    "tab.inactiveForeground": "#43a241cb",
    "tab.activeBackground": "#042f5c",
    "tab.inactiveBackground": "#110f1b",
    "statusBar.background": "#15202d",
    "statusBar.foreground": "#67808d",
    "statusBarItem.hoverBackground": "#081b2f",
    "statusBarItem.prominentForeground": "#ffffff",
    "statusBarItem.errorForeground": "#ff0000",
    "statusBarItem.hoverForeground": "#00ff1a",
    "sideBar.background": "#04131d",
    "sideBarSectionHeader.foreground": "#fefefe",
    "sideBarSectionHeader.background": "#001e79c2",
    "activityBarBadge.background": "#007ACC",
    "activityBarBadge.foreground": "#FFFFFF",
    "activityBar.activeBorder": "#00a6ff",
    "activityBar.activeBackground": "#1a299c",
    "activityBar.inactiveForeground": "#8abcf1",
    "sideBarTitle.foreground": "#00a6ff",
    "activityBar.foreground": "#2890ff",
    "menu.foreground": "#00b4fb",
    "menu.background": "#04131d",
    "menu.selectionForeground": "#75b214",
    "menu.selectionBackground": "#223fd0",
    "sideBar.foreground": "#a2c2e4",
    "list.errorForeground": "#00ff0d",
    "list.focusForeground": "#01ff34",
    "list.activeSelectionForeground": "#01ff34",
    "list.hoverForeground": "#01ff34",
    "list.highlightForeground": "#01ff34",
    "list.hoverBackground": "#2d2863",
    "list.activeSelectionBackground": "#2a3b85",
    "list.inactiveSelectionBackground": "#241d67",
    "list.inactiveSelectionForeground": "#02ff13",
    "gitDecoration.modifiedResourceForeground": "#ff5100",
    "gitDecoration.ignoredResourceForeground": "#2d80cd",
    "menu.border": "#1949d9",
    "banner.foreground": "#01ff34",
    "foreground": "#00a6ff",
    "input.foreground": "#7bb8d4",
    "menubar.selectionForeground": "#00ff1ec0",
    "menu.separatorBackground": "#16276c",
    "menubar.selectionBackground": "#101760",
    "menubar.hoverForeground": "#ff0000",
    "menubar.hoverBackground": "#00ff00",
    "titleBar.activeForeground": "#00a6ff",
    "titleBar.inactiveForeground": "#0084ff",
    "editorGroup.border": "#1b2373",
    "editor.background": "#021524"
  },
  "editor.linkedEditing": true,
  "notebook.stickyScroll.enabled": true,
  "workbench.startupEditor": "none",
  "window.systemColorTheme": "dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.autoLockGroups": {
    "vscode.audioPreview": true
  },
  "explorer.confirmDragAndDrop": false,
  "editor.scrollBeyondLastLine": false,
  "diffEditor.renderSideBySide": true,
  "diffEditor.renderIndicators": false,
  "git.confirmSync": false,
  "editor.scrollbar.vertical": "hidden",
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "explorer.confirmDelete": false,
  "emmet.triggerExpansionOnTab": true,
  "terminal.integrated.tabs.focusMode": "singleClick",
  "window.title": "${activeEditorMedium}",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.openPositioning": "last",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "git.openRepositoryInParentFolders": "never",
  // Para las líneas verticales en el explorador de archivos
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 4,
  "editor.indentSize": 4,
  "editor.detectIndentation": true,
  "workbench.sideBar.location": "right",
  "workbench.secondarySideBar.showLabels": false,
  "editor.autoClosingBrackets": "always",
  "window.commandCenter": false,
  "workbench.colorTheme": "Cobalt2 Theme Tweaked",
  "editor.experimentalGpuAcceleration": "on",
  "css.format.spaceAroundSelectorSeparator": true,
  "less.format.spaceAroundSelectorSeparator": true,
  "scss.format.spaceAroundSelectorSeparator": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "files.trimTrailingWhitespace": true,
  "editor.fontFamily": "Consolas, monospace",
  "editor.wordWrap": "on",
  "editor.padding.top": 25,
  "editor.padding.bottom": 25,
  "scm.diffDecorationsIgnoreTrimWhitespace": "true",
  "workbench.sash.size": 1,
  "comments.openView": "file",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollBeyondLastColumn": 0,
  "editor.semanticHighlighting.enabled": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.maxLineCount": 10,
  "editor.unfoldOnClickAfterEndOfLine": true,
  "editor.unicodeHighlight.includeComments": false,
  "workbench.editor.decorations.colors": false,
  "explorer.decorations.colors": false,
  "editor.renderFinalNewline": "off",
  "css.format.preserveNewLines": false,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.tabSizingFixedMaxWidth": 120,
  "workbench.editor.tabSizingFixedMinWidth": 120,
  "workbench.editor.titleScrollbarSizing": "large",
  "html.format.wrapAttributes": "force-expand-multiline",
  "editor.tabCompletion": "on",
  "explorer.sortOrder": "filesFirst",
  "explorer.sortOrderLexicographicOptions": "upper",
  "explorer.sortOrderReverse": true,
  "editor.multiCursorPaste": "full",
  "editor.lineHeight": 0,
  "editor.lineNumbers": "on",
  "editor.showFoldingControls": "mouseover",
  "editor.glyphMargin": false,
  "editor.folding": true,
  "html.format.wrapLineLength": 0,
  "prettier.proseWrap": "never",
  "notebook.output.wordWrap": true,
  "editor.wordWrapColumn": 105,
  "diffEditor.wordWrap": "on",
  "workbench.editor.wrapTabs": true,
  "chat.editor.wordWrap": "on",
  "editor.unusualLineTerminators": "auto",
  "zenMode.hideLineNumbers": false,
  "editor.guides.highlightActiveIndentation": false,
  "editor.unicodeHighlight.includeStrings": "inUntrustedWorkspace",
  "typescript.format.enable": false,
  "typescript.format.semicolons": "insert",
  "reactSnippets.settings.prettierEnabled": true,
  "prettier.endOfLine": "auto",
  "editor.autoClosingDelete": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "breadcrumbs.enabled": false,
  "window.density.editorTabHeight": "compact",
  "editor.columnSelection": true,
  "notebook.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "notebook.formatOnCellExecution": true,
  "notebook.formatOnSave.enabled": true,
  "indentRainbow.colorOnWhiteSpaceOnly": true,
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.colors": [
    "rgba(255,255,64,0.07)",
    "rgba(127,255,127,0.07)",
    "rgba(255,127,255,0.07)",
    "rgba(79,236,236,0.07)"
  ],
  "editor.codeLensFontFamily": "Consolas, monospace",
  "editor.wrappingIndent": "none",
  "prettier.vueIndentScriptAndStyle": true,
  "editor.tabSize": 2,
  "editor.defaultFoldingRangeProvider": "VisualStudioExptTeam.vscodeintellicode",
  "notebook.stickyScroll.mode": "flat",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": false,
  "editor.hover.above": false,
  "editor.guides.indentation": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.inlayHints.padding": true,
  "editor.renderLineHighlight": "all",
  "editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
  "editor.autoClosingComments": "always",
  "editor.stickyTabStops": true,
  "editor.cursorSmoothCaretAnimation": "explicit",
  "terminal.integrated.accessibleViewPreserveCursorPosition": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.overtypeCursorStyle": "underline",
  "editor.cursorStyle": "underline",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "underline",
  "editor.suggest.localityBonus": true
}
